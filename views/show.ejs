<% include ./partials/header.ejs%>
    <div class="w-full min-h-screen bg-white px-10 py-8 text-black">
        <div class="cards start flex flex-wrap items-start gap-10">
            <% user.posts.forEach(function(elem) { %>
                <div class="card flex flex-col items-start ">
                    <div class="min-w-32 max-w-[220px] bg-sky-200 rounded-lg overflow-hidden">
                        <img src="/images/uploads/<%= elem.image %>" class="w-full h-full object-cover" alt="">
                    </div>
                    <h5 class="mt-2 text-s"><%= elem.Title %></h5>
                </div>
            <% }) %>
            
        </div>
    </div>
<% include ./partials/footer.ejs%>